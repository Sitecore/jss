  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "HTTP Renderer Start",
      "env": {
        "NODE_ENV": "development",
        "JSS_RENDER_ENGINE": "http"
      },
      "program": "${workspaceFolder}/scripts/http-renderer.js",
      "runtimeArgs": ["--preserve-symlinks", "--preserve-symlinks-main"]
    }
  ]
}

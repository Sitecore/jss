{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "target": "es2016",
    "module": "commonjs",
    "newLine": "LF",
    "types": []
  },
  "lib": [
    "esnext.asynciterable"
  ],
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "**/*.sitecore.ts",
    "./main.ts",
    "./polyfills.ts",
    "./environments/environment.prod.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule"
  }
}

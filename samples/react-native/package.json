{
  "name": "jss-sample-reactnative",
  "version": "14.0.3",
  "description": "A basic React Native app utilizing Sitecore JavaScript Services",
  "config": {
    "appName": "JssReactNative",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "language": "en"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "scripts": {
    "jss": "jss",
    "start-android": "node ./build/start.js --platform android",
    "start-android:connected": "scjss-verify-setup && node ./build/start.js --platform android --appMode connected",
    "start-android:connected-tunnel": "scjss-verify-setup && node ./build/start.js --platform android --appMode connected-tunnel",
    "start-ios": "node ./build/start.js --platform ios",
    "start-ios:connected": "scjss-verify-setup && node ./build/start.js --platform ios --appMode connected",
    "start-ios:connected-tunnel": "scjss-verify-setup && node ./build/start.js --platform ios --appMode connected-tunnel",
    "lint": "eslint --fix ./src/**/*.js ./sitecore/definitions/**/*.js ./**/*.js"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss-react-native": "^14.0.3",
    "axios": "^0.18.1",
    "prop-types": "^15.6.0",
    "react": "^16.9.0",
    "react-native": "^0.63.3",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-svg": "^9.13.3",
    "react-native-svg-uri": "^1.2.3",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3"
  },
  "private": true,
  "devDependencies": {
    "@sitecore-jss/sitecore-jss-cli": "^14.0.3",
    "@sitecore-jss/sitecore-jss-dev-tools": "^14.0.3",
    "@sitecore-jss/sitecore-jss-manifest": "^14.0.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-define": "^1.3.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "4.0.0",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.3",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.2.1",
    "ngrok": "^3.0.1",
    "prettier": "^1.14.2"
  },
  "optionalDependencies": {
    "ttab": "^0.6.1"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "homepage": "https://jss.sitecore.net",
  "license": "Apache-2.0"
}

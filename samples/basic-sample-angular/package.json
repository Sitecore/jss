{
  "name": "basic-app-angular",
  "version": "7.0.0",
  "description": "A basic Angular app utilizing Sitecore JavaScript Services.",
  "config": {
    "appName": "JssBasicAppAngular",
    "sitecoreDistPath": "/dist/JssBasicAppAngular",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "buildArtifactsPath": "./dist",
    "language": "en"
  },
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "jss": "jss",
    "start": "ng serve --base-href=/",
    "start:connected": "echo Connected mode is not yet supported with angular basic sample.",
    "build": "ng build --prod && ng build --prod --app 1 --output-hashing=false && npm run build:webpack-server",
    "build:webpack-server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "author": {
    "name": "Sitecore Corporation",

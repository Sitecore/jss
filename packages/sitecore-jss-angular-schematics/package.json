  "homepage": "https://jss.sitecore.com",
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "~17.3.8",
    "@angular-devkit/schematics": "~17.3.8",
    "@schematics/angular": "~17.3.8",
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.0",
    "@types/node": "^20.14.2",
    "jasmine": "^2.99.0",

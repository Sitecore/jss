    "coverage": "nyc npm run test-package",
    "generate-docs": "npx typedoc --plugin typedoc-plugin-markdown --readme none --out ../../ref-docs/sitecore-jss-angular-schematics src/jss-component/index.ts --githubPages false"
  },
  "engines": {
    "node": ">=20"
  },

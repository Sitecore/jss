    "babel-core": "^6.26.3",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.33.0",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",

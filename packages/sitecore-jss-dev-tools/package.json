    "del": "^6.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "import-fresh": "^3.3.0",
    "joi": "^17.7.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "module-alias": "^2.2.2",
    "readline-sync": "^1.4.10",
    "resolve": "^1.22.1",
    "ts-node": "^10.9.1",
    "url-join": "^4.0.1",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",

{
  "extension": [
    ".ts"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.test.ts",
    "src/**/test-data",
    "src/pipelines/test",
    "dist",
    "types",
    "./src/manifest/index.ts",
    "./src/pipelines/index.ts",
    "./src/templating/index.ts",
    "./src/templating/nextjs/index.ts",
    "./src/templating/nextjs/constants.ts",
    "./src/bin/*.ts",
    "./src/manifest/generator/SitecoreIcon.ts",
    "./src/manifest/generator/manifest.types*.ts",
    "./src/manifest/generator/manifest.babel-shim.ts",
    "**/pipeline.config.ts"
  ],
  "all": true,
  "reporter": [
    "cobertura",
    "text"
  ],
  "require": ["ts-node/register"]
}

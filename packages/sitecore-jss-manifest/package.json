    "@types/uuid": "^3.4.4",
    "@types/yargs": "^12.0.1",
    "babel-core": "^6.26.3",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "cpx": "^1.5.0",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "rewire": "^4.0.1",
    "sinon": "^7.0.0",
    "typescript": "^3.1.3"

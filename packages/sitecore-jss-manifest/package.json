  "homepage": "https://jss.sitecore.net",
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss": "^12.0.0",
    "@sitecore-jss/sitecore-pipelines": "^12.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "import-fresh": "^2.0.0",
    "joi": "^14.0.0",
    "ts-node": "^7.0.1",
    "uuid": "^3.3.2",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",

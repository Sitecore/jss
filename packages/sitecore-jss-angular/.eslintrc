{
  "extends": [
    "plugin:@angular-eslint/recommended",
    "../../.eslintrc",
    "../../eslint-configs/typescript"
	],
  "plugins": [
    "deprecation"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "deprecation/deprecation": "warn"
  }
}

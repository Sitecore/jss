{
  "name": "@sitecore-jss/sitecore-jss-cli",
  "version": "12.0.0",
  "description": "Sitecore JSS command-line",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "npm run build -- --watch",
    "clean": "del-cli dist types",
    "lint": "tslint -p . -c ../../tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "jss": "node ./dist/bin/jss.js",
    "test": "mocha --require ts-node/register \"./src/**/*.test.ts\"",
    "coverage": "nyc --require ts-node/register npm test"
  },
  "engines": {
    "node": ">=8.1"
  },
  "preferGlobal": true,
  "bin": {
    "jss": "./dist/bin/jss.js"
  },
  "main": "./dist/cli.js",
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.net"
  },
  "license": "Apache-2.0",
  "homepage": "https://jss.sitecore.net",
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss-dev-tools": "^12.0.0",
    "@sitecore-jss/sitecore-jss-manifest": "^12.0.0",
    "chalk": "^2.4.1",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "jszip": "^3.1.5",
    "readline-sync": "^1.4.9",
    "request": "^2.88.0",
    "resolve": "^1.8.1",
    "tmp": "^0.0.33",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/cross-spawn": "^6.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/jszip": "^3.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^9.6.1",
    "@types/readline-sync": "^1.4.3",
    "@types/request": "^2.47.1",
    "@types/resolve": "^0.0.8",
    "@types/sinon": "^5.0.5",
    "@types/tmp": "^0.0.33",
    "@types/yargs": "^12.0.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "sinon": "^7.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "gitHead": "2f4820efddf4454eeee58ed1b2cc251969efdf5b"
}

{
  "name": "@sitecore-jss/sitecore-jss-cli",
  "version": "16.0.0-canary.5",
  "description": "Sitecore JSS command-line",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "npm run build -- --watch",
    "clean": "del-cli dist types",
    "lint": "tslint -p . -c ../../tslint.json \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "jss": "node ./dist/bin/jss.js",
    "test": "mocha --require ts-node/register \"./src/**/*.test.ts\"",
    "coverage": "nyc --require ts-node/register npm test"
  },
  "engines": {
    "node": ">=8.1"
  },
  "preferGlobal": true,
  "bin": {
    "jss": "./dist/bin/jss.js"
  },
  "main": "./dist/cli.js",
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.net"
  },
  "license": "Apache-2.0",
  "homepage": "https://jss.sitecore.net",
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss-dev-tools": "^16.0.0-canary.5",
    "@sitecore-jss/sitecore-jss-manifest": "^16.0.0-canary.5",
    "chalk": "^2.4.2",
    "cross-spawn": "^7.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "jszip": "^3.2.2",
    "readline-sync": "^1.4.10",
    "request": "^2.88.0",
    "resolve": "^1.12.0",
    "tmp": "^0.1.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "^7.1.1",
    "@types/jszip": "^3.1.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/readline-sync": "^1.4.3",
    "@types/request": "^2.48.3",
    "@types/resolve": "^0.0.8",
    "@types/sinon": "^7.5.0",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.3",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.1",

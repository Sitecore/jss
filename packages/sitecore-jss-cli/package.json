{
  "name": "@sitecore-jss/sitecore-jss-cli",
  "version": "20.0.0-canary.55",
  "description": "Sitecore JSS command-line",
  "main": "dist/cjs/cli.js",
  "module": "dist/esm/cli.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-esm.json && npm run build-templates",
    "build-templates": "ts-node ./build-templates.ts",
    "watch": "npm run build -- --watch",
    "clean": "del-cli dist types",
    "lint": "eslint ./src/**/*.ts",
    "prepublishOnly": "npm run build",
    "jss": "node ./dist/cjs/bin/jss.js",
    "test": "mocha --require ts-node/register \"./src/**/*.test.ts\"",
    "coverage": "nyc npm test"
  },
  "engines": {
    "node": ">=8.1"
  },
  "preferGlobal": true,
  "bin": {
    "jss": "./dist/cjs/bin/jss.js"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://jss.sitecore.com",
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss-dev-tools": "^20.0.0-canary.55",
    "axios": "^0.21.1",
    "chalk": "^2.4.2",
    "cross-spawn": "^7.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.1.6",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "inquirer": "^8.2.0",
    "jszip": "^3.2.2",
    "readline-sync": "^1.4.10",
    "resolve": "^1.12.0",
    "tmp": "^0.1.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^8.1.3",
    "@types/jszip": "^3.1.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/readline-sync": "^1.4.3",
    "@types/resolve": "^0.0.8",
    "@types/sinon": "^7.5.0",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.3",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.15.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "typescript": "~4.3.5"
  },
